<div class="field">
		  <%= f.attribute_fields do |a| %>
		    <%= a.attribute_select({}, :class => 'attribute') %>
		  <% end %>
		  <%= f.predicate_select({}, :class => 'attribute') %>
		  <%= f.value_fields do |v| %>
		    <%= v.text_field :value %>
		  <% end %>
		  <%= link_to "remove", '#', class: "remove_fields" %>

</div>
